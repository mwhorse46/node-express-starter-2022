{
  "name": "node-express-starter-2022",
  "version": "1.1.0",
  "description": "A node and express starter template for 2022",
  "author": "Carter Adams <carteradams@protonmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist --copy-files",
    "start": "node dist/index.js",
    "dev": "babel-watch -w .env -w src src/index.js",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "lint": "eslint src"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/node": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@types/jest": "^28.1.3",
    "babel-watch": "^7.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "jest": "^28.1.3",
    "regenerator-runtime": "^0.13.9",
    "supertest": "^6.2.4"
  }
}
