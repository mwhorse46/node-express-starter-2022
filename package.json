{
  "name": "node-express-starter-2022",
  "version": "0.2.1",
  "description": "A minimally opinionated node & express starter for 2022",
  "author": "Carter Adams",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --verbose",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "dev": "nodemon --delay 500ms -r dotenv/config -w .env -w src src/index.js --exec babel-node",
    "build": "babel src -d dist --copy-files",
    "start": "node dist/index.js"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babel-plugin-module-resolver": "^4.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "regenerator-runtime": "^0.13.9",
    "supertest": "^6.2.2"
  }
}
